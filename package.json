    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "requirejs": "^2.3.6",
    "webpack": "3.1.0",
    "yarn-install": "^1.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "babel-cli": "^6.26.0",
    "css-loader": "^2.1.0",
    "style-loader": "^0.23.1"
  }
